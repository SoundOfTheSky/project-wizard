{
  "name": "b",
  "version": "1.0.0",
  "license": "MIT",
  "dependencies": {
    "vue": "next",
    "vue-router": "next",
    "vuex": "next"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "@vitejs/plugin-vue": "latest",
    "@vue/compiler-sfc": "latest",
    "eslint": "latest",
    "eslint-config-prettier": "latest",
    "eslint-plugin-prettier": "latest",
    "eslint-plugin-vue": "latest",
    "prettier": "latest",
    "sass": "^1",
    "stylelint": "latest",
    "stylelint-config-prettier": "latest",
    "stylelint-config-rational-order": "latest",
    "stylelint-config-recommended-scss": "latest",
    "stylelint-config-standard": "latest",
    "stylelint-order": "latest",
    "stylelint-prettier": "latest",
    "stylelint-scss": "latest",
    "typescript": "latest",
    "vite": "latest",
    "vue-eslint-parser": "latest",
    "vue-tsc": "latest"
  },
  "scripts": {
    "lint": "prettier -c \"**/*{.js,.json,.jsx,.ts,.tsx}\" && eslint \"src/**/*{.js,.jsx,.ts,.tsx}\" && stylelint \"src/**/*{.css,.scss}\"",
    "lint:fix": "prettier -w \"**/*{.js,.json,.jsx,.ts,.tsx}\" && eslint \"src/**/*{.js,.jsx,.ts,.tsx}\" --fix && stylelint \"src/**/*{.css,.scss}\" --fix",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  }
}
