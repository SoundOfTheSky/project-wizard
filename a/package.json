{
  "name": "a",
  "version": "1.0.0",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "prettier": "latest",
    "eslint": "latest",
    "eslint-plugin-prettier": "latest",
    "eslint-config-prettier": "latest",
    "stylelint": "latest",
    "stylelint-config-rational-order": "latest",
    "stylelint-config-standard": "latest",
    "stylelint-order": "latest",
    "stylelint-prettier": "latest",
    "stylelint-config-prettier": "latest",
    "vite": "^2",
    "esbuild": "latest",
    "electron": "latest",
    "electron-builder": "latest"
  },
  "scripts": {
    "lint": "prettier -c \"**/*.json\" && eslint \"src/**/*{.js,.jsx}\" && stylelint \"src/**/*.css\"",
    "lint:fix": "prettier -w \"**/*.json\" && eslint \"src/**/*{.js,.jsx}\" --fix && stylelint \"src/**/*.css\" --fix",
    "dev": "node scripts/dev",
    "build": "node scripts/build --build",
    "preview": "electron dist",
    "compile:linux": "node scripts/build -l",
    "compile:mac": "node scripts/build -m",
    "compile:windows": "node scripts/build -w"
  }
}
